        "parameter_ref": "fields_filter_measurement",
        "label": {
          "default": "Filter by Measurement"
        },
        "description": {
          "default": "Filters out all fields without a 'measurement' of 'discrete'.  Should be all 'BP*' fields."
        }
      },
      {
        "parameter_ref": "fields_filter_type",
        "label": {
          "default": "Filter by Type"
        },
        "description": {
          "default": "Filters out all fields without a 'type' of 'integer'.  Should be all 'age*' fields."
        }
      },
      {
        "parameter_ref": "fields_filter_roles",
        "label": {
          "default": "Filter by Modeling Roles"
        },
        "description": {
          "default": "Filters out all fields without a 'modeling_role' of 'target' or 'both' using 'values'.  Should be all 'drug*' and 'age*' and  fields."
        }
      },
      {
        "parameter_ref": "fields_filter_and",
        "label": {
          "default": "Filter by Type and Measurement"
        },
        "description": {
          "default": "Filters out all fields without a 'type' of 'string' and 'measurement' of 'set'.  Should be all 'drug*' fields."
        }
      },
      {
        "parameter_ref": "fields_filter_or",
        "label": {
          "default": "Filter by Type or Measurement"
        },
        "description": {
          "default": "Filters out all fields without a 'type' of 'integer' or 'measurement' of 'set'.  Should be all 'drug*' and 'age*' fields."
        }
      },
