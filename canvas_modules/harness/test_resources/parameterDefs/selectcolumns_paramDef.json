  "conditions": [
    {
      "validation": {
        "fail_message": {
          "type": "error",
          "focus_parameter_ref": "fields_error",
          "message": {
            "default": "Don't select 'age'"
          }
        },
        "evaluate": {
          "condition": {
            "parameter_ref": "fields_error",
            "op": "notContains",
            "value": "age"
          }
        }
      }
    },
    {
      "validation": {
        "fail_message": {
          "type": "warning",
          "focus_parameter_ref": "fields_warning",
          "message": {
            "default": "Shouldn't select 'age'"
          }
        },
        "evaluate": {
          "condition": {
            "parameter_ref": "fields_warning",
            "op": "notEquals",
            "value": [
              "age"
            ]
          }
        }
      }
    },
    {
      "visible": {
        "parameter_refs": [
          "fields_hidden"
        ],
        "evaluate": {
          "condition": {
            "parameter_ref": "hide",
            "op": "equals",
            "value": false
          }
        }
      }
    },
    {
      "enabled": {
        "parameter_refs": [
          "fields_disabled"
        ],
        "evaluate": {
          "condition": {
            "parameter_ref": "disable",
            "op": "equals",
            "value": false
          }
        }
      }
    },
    {
      "filter": {
        "parameter_ref": "fields_filter_type",
        "evaluate": {
          "condition": {
            "op": "dmType",
            "value": "integer"
          }
        }
      }
    },
    {
      "filter": {
        "parameter_ref": "fields_filter_measurement",
        "evaluate": {
          "condition": {
            "op": "dmMeasurement",
            "value": "discrete"
          }
        }
      }
    },
    {
      "filter": {
        "parameter_ref": "fields_filter_roles",
        "evaluate": {
          "condition": {
            "op": "dmModelingRole",
            "values": [
              "target",
              "both"
            ]
          }
        }
      }
    },
    {
      "filter": {
        "parameter_ref": "fields_filter_and",
        "evaluate": {
          "and": [
            {
              "condition": {
                "op": "dmMeasurement",
                "value": "set"
              }
            },
            {
              "condition": {
                "op": "dmType",
                "value": "string"
              }
            }
          ]
        }
      }
    },
    {
      "filter": {
        "parameter_ref": "fields_filter_or",
        "evaluate": {
          "or": [
            {
              "condition": {
                "op": "dmMeasurement",
                "value": "set"
              }
            },
            {
              "condition": {
                "op": "dmType",
                "value": "integer"
              }
            }
          ]
        }
      }
    }
  ],
